        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="GrupoAlunoDisciplinasCursadas">
    <xs:sequence minOccurs="0" maxOccurs="unbounded">
      <xs:element name="alunoDisciplinasCursadas" type="ItemAlunoDisciplinasCursadas">
        <xs:annotation>
          <xs:documentation>Representa as disciplinas cursadas para um dado aluno.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="GrupoAmbiente">
    <xs:sequence minOccurs="0" maxOccurs="unbounded">
      <xs:element name="ambiente" type="ItemAmbiente">
        <xs:annotation>
          <xs:documentation>Representa um ambiente (isto é, uma sala de aula ou laboratório).</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="GrupoCampus">
    <xs:sequence minOccurs="0" maxOccurs="unbounded">
      <xs:element name="campus" type="ItemCampus">
