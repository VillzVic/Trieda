        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="GrupoOfertaCampus">
    <xs:sequence minOccurs="0" maxOccurs="unbounded">
      <xs:element name="ofertaCampus" type="ItemOfertaCampus">
