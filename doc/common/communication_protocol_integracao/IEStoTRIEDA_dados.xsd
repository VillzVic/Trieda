  <xs:complexType name="ItemProfessor">
    <xs:sequence>
      <xs:element name="cpf" type="xs:string">
        <xs:annotation>
          <xs:documentation>CPF do professor que será utilizado como identificador único do mesmo.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="nome" type="xs:string">
        <xs:annotation>
          <xs:documentation>Nome do professor, por exemplo, "Fulano".</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="tipoContratoId" type="xs:int">
        <xs:annotation>
          <xs:documentation>Inteiro que identifica do tipo de contrato do professor em questão. Atualmente as possíveis opções são: 1-Parcial, 2-Integral.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="chMin" type="xs:int">
        <xs:annotation>
          <xs:documentation>Carga horária mínima do professor na semana. Este campo é dado em quantidade de tempos de aula (ou créditos), por exemplo, 3 (no mínimo 3 tempos de aula na semana).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="chMax" type="xs:int">
        <xs:annotation>
          <xs:documentation>Carga horária máxima do professor na semana. Este campo é dado em quantidade de tempos de aula (ou créditos), por exemplo, 12 (no máximo 12 tempos de aula na semana).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="chAnterior" type="xs:int">
        <xs:annotation>
          <xs:documentation>Carga horária semanal do professor no período letivo anterior.  Este campo é dado em quantidade de tempos de aula (ou créditos), por exemplo, 20 (20 tempos de aula por semana no período anterior).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="titulacaoId" type="xs:int">
        <xs:annotation>
          <xs:documentation>Inteiro que identifica o tipo de titulação do professor em questão. Atualmente, os possíveis valores são: 1-Licenciado, 2-Bacharel, 3-Especialista, 4-Mestre, 5-Doutor.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="custoCredito" type="xs:double">
        <xs:annotation>
          <xs:documentation>Valor (em reais) do crédito pago (ou 1 tempo de aula pago) ao professor em questão.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="maxDiasSemana" type="xs:int">
        <xs:annotation>
          <xs:documentation>Quantidade máxima de dias por semana que o professor está disposto a ir na instituição para ministrar aulas.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="minCreditosDiarios" type="xs:int">
        <xs:annotation>
          <xs:documentation>Quantidade mínima de créditos (ou tempos de aula) por dia que o professor aceita para ir na instituição para ministrar aulas.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="disponibilidades" type="GrupoDisponibilidade">
        <xs:annotation>
          <xs:documentation>Lista com os horários disponíveis do professor em questão.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ItemProfessorCampus">
    <xs:sequence>
      <xs:element name="professorCpf" type="xs:string">
        <xs:annotation>
          <xs:documentation>CPF do professor (ItemProfessor).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="campusId" type="xs:string">
        <xs:annotation>
          <xs:documentation>Identificador do campus (ItemCampus) no qual o professor (ItemProfessor) é pode ministrar aulas.</xs:documentation>
