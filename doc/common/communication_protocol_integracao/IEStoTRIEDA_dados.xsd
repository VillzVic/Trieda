  <xs:complexType name="GrupoOfertaCurriculo">
    <xs:sequence minOccurs="0" maxOccurs="unbounded">
      <xs:element name="ofertaCurriculo" type="ItemOfertaCurriculo">
        <xs:annotation>
          <xs:documentation>Representa uma oferta de uma matriz curricular.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="GrupoOfertaCurso">
    <xs:sequence minOccurs="0" maxOccurs="unbounded">
      <xs:element name="ofertaCurso" type="ItemOfertaCurso">
